@using MapWebSite.Resources.text
@{
    Layout = null;
}

<div class="points-settings-layer-container-palette-content points-settings-layer-container-content">
    <div class="ps_header">
        <h4>@Html.Raw(TextDictionary.HPSCDTitleText)</h4>
        @Html.DropDownList("filter",
                         EnumHelper.GetSelectList(typeof(MapWebSite.Core.Database.DataPointsFilters))
                         )

        <input type="search" id="colorPaletteSearchValue" placeholder="@TextDictionary.HPSCDSearchInput" onkeyup="loadMorePalettes(true)" />

    </div>
    <div class="ps_left" id="ps_left">

    </div>
    <aside class="ps_right"></aside>
</div>
