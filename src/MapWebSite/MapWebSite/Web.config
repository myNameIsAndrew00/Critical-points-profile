<configuration>

  <configSections>
    <sectionGroup name="bundleTransformer">
      <section name="core" type="BundleTransformer.Core.Configuration.CoreSettings, BundleTransformer.Core" />
      <section name="sassAndScss" type="BundleTransformer.SassAndScss.Configuration.SassAndScssSettings, BundleTransformer.SassAndScss" />
    </sectionGroup>
  </configSections>
  <appSettings>
    <add key="webpages:Version" value="3.0.0.0" />
    <add key="webpages:Enabled" value="false" />
    <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />
    <add key="CassandraServer" value="127.0.0.1" />
    <add key="CassandraKeyspace" value="usersdatapoints" />

    <!--Entries below are used to control the access to geoserver rest api-->
    <add key="GeoserverApiUrl" value="http://localhost:8080"/>
    <add key="GeoserverUser" value="admin"/>
    <add key="GeoserverPassword" value="geoserver"/>
    
    <!--This entry must be the same with the one inside DataPointsParserService configuration file-->
    <add key="DataPointsSourceFileName" value="__OK" />

    <!--This path must be the same with the one inside DataPointsParserService configuration file-->
    <add key="PointsDatasetsCheckpointFolder" value="E:\Application Service Files\Files Checkpoint" />

    <!--This represents the api key used for email azure service (SendGrid https://app.sendgrid.com/guide/integrate/langs/csharp ) -->
    <add key="ApiKey" value="placeholder_for_api_key" />

    <!-- Email address visible for email confirmation sender -->
    <add key="EmailAddress" value="noreply.pstool@contact" />
    
    <!-- Display name visible when a confirmation email is send-->
    <add key="EmailDisplayName" value="PSTool No Reply" />
    
  </appSettings>
  <connectionStrings>
    <add name="PersistantScatterersDatabase" connectionString="Data Source=(local); Initial Catalog=PersistantScatterersDatabase; Integrated Security=SSPI;Connection Timeout=200" />
  </connectionStrings>
  <system.web>
    <compilation debug="true" targetFramework="4.7.2">
      <assemblies>
        <add assembly="netstandard, Version=2.0.0.0, Culture=neutral, PublicKeyToken=cc7b13ffcd2ddd51" />
      </assemblies>
    </compilation>
    <httpRuntime targetFramework="4.7.2" maxRequestLength="2147483647" executionTimeout="1600" requestLengthDiskThreshold="2147483647" />

  </system.web>
